@using TeaShopApi.WebUI.Dtos.ChefsDtos
@model CreateChefsDto
@{
    ViewData["Title"] = "CreateChefs";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}

<h1>Yeni Şef Girişi</h1>

<form method="post">
    <label>Ad Soyad</label>
    <input type="text" asp-for="NameSurname" class="form-control" />
    <br />
    <label>Açıklama</label>
    <input type="text" asp-for="Description" class="form-control" />
    <br />
    <label>Unvan</label>
    <input type="text" asp-for="Unvan" class="form-control" />
    <br />
    <label>Resim</label>
    <input type="text" asp-for="ImageUrl" class="form-control" />
    <br />
   

    <button class="btn btn-primary">Ekle</button>
</form>
