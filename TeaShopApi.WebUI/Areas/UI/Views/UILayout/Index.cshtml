
@{
    Layout = null;
    var actionUrl = Url.Action("PartialHero", "UILayout");

}

<!DOCTYPE html>
<html lang="en">

@Html.Partial("PartialHead","UILayout") 

<body>


 @Html.Partial("PartialHeader","UILayout")

    @await Component.InvokeAsync("_PartialHero")

    @await Component.InvokeAsync("_PartialAboutus")

    <!-- ======= Why Us Section ======= -->
    @await Component.InvokeAsync("_PartialWhy")

    <!-- ======= Stats Counter Section ======= -->
    @Html.Partial("PartialStats","UILayout")

    <!-- ======= Menu Section ======= -->
    @await Component.InvokeAsync("_PartialMenu")

    <!-- ======= Testimonials Section ======= -->
    @await Component.InvokeAsync("_PartialTest")
    @RenderBody()

    <!-- ======= Events Section ======= -->
    @await Component.InvokeAsync("_PartialEvent")

    <!-- ======= Chefs Section ======= -->
    @await Component.InvokeAsync("_PartialChef")
    <!-- ======= Book A Table Section ======= -->
    @Html.Partial("PartialBook","UILayout")

    <!-- ======= Gallery Section ======= -->
    @await Component.InvokeAsync("_PartialGallery")

    <!-- ======= Contact Section ======= -->
    @Html.Partial("PartialContact","UILayout")

 <!-- End #main -->

  <!-- ======= Footer ======= -->
    @Html.Partial("PartialFooter","UILayout")

  <!-- End Footer -->

  <a href="#" class="scroll-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

  <div id="preloader"></div>

  <!-- Vendor JS Files -->
    <script src="~/Yummy/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/Yummy/assets/vendor/aos/aos.js"></script>
    <script src="~/Yummy/assets/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="~/Yummy/assets/vendor/purecounter/purecounter_vanilla.js"></script>
    <script src="~/Yummy/assets/vendor/swiper/swiper-bundle.min.js"></script>
    <script src="~/Yummy/assets/vendor/php-email-form/validate.js"></script>

  <!-- Template Main JS File -->
    <script src="~/Yummy/assets/js/main.js"></script>

</body>

</html>